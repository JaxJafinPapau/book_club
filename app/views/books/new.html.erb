<%= form_for @book do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :pub_year %>
  <%= f.number_field :pub_year %>
  <%= f.label :page_count %>
  <%= f.number_field :page_count %>
  <%= f.label :thumbnail_url %>
  <%= f.text_field :thumbnail_url %>
  <%= f.label :authors %>
  <%= f.text_field :authors %>
  <%= f.submit %>
<% end %>
